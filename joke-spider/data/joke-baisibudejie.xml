<?xml version="1.0" encoding="utf-8"?>
<xml>
    <xsl:for-each select="//div[class='main']/div/div/div">
      <joke>
      	<id>
      	  <xsl:attr-of select="//p[class='web_size']" attr='id' />
      	</id>
  	    <author>
          <xsl:value-of select="//div[class='web_msg']/p/a" />
        </author>
        <datetime>
          <xsl:value-of select="//div[class='web_msg']/p/span" />
        </datetime>
        <contents>
          <xsl:for-each select="//p[class='web_size']/$">
        	<content>
        		<xsl:value-of select="." />
        	</content>
          </xsl:for-each>
        </contents>
        <image>
        	<xsl:attr-of select="//p[class='web_pic']/a/img" attr='src' />
        </image>
        <approve>
        	<xsl:value-of select="//div[class='web_column']/ul/li/a/span" />
        </approve>
        <disapprove>
        	<xsl:value-of select="//div[class='web_column']/ul/li[2]/a/span" />
        </disapprove>
        <comment>
        	<xsl:value-of select="//div[class='web_column']/ul[2]/li/a/em" />
        </comment>
      </joke>
    </xsl:for-each>
</xml>
